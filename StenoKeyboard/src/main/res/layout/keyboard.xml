<?xml version="1.0" encoding="utf-8"?>

<com.brentandjody.stenokeyboard.TouchLayer xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:id="@+id/keyboard"
    android:background="@android:color/darker_gray"
    android:measureWithLargestChild="false">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:paddingTop="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/S"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:cropToPadding="true"
                android:padding="10dp"
                android:paddingLeft="20dip"
                android:paddingRight="30dip"
                android:clickable="false"
                android:text="S"
                android:hint="S-" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:id="@+id/linearLayout"
            android:layout_weight="@integer/key_weight">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/T"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:clickable="false"
                android:text="T"
                android:layout_weight="1"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:hint="T-" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"></FrameLayout>

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/K"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:clickable="false"
                android:text="K"
                android:layout_weight="1"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:hint="K-" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:id="@+id/linearLayout2"
            android:layout_weight="@integer/key_weight">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/P"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="P"
                android:hint="P-" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout10" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/W"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:clickable="false"
                android:text="W"
                android:layout_weight="1"
                android:hint="W-" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:id="@+id/linearLayout3"
            android:layout_weight="@integer/key_weight">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/H"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="H"
                android:hint="H-" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout11" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/R"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:clickable="false"
                android:text="R"
                android:layout_weight="1"
                android:hint="R-" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight"
            android:id="@+id/linearLayout4">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:text="#"
                android:id="@+id/number_bar"
                android:background="@drawable/numberbar"
                android:clickable="false"
                android:layout_weight="1"
                android:layout_marginLeft="2dp"
                android:hint="#"
                android:layout_marginRight="2dp" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"></FrameLayout>

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:text="A"
                android:id="@+id/A"
                android:layout_centerInParent="true"
                android:background="@drawable/vowelkey"
                android:clickable="false"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:hint="A-" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight"
            android:id="@+id/linearLayout11">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:layout_marginBottom="40dp"
                android:layout_marginLeft="8dp"
                android:background="@android:color/black"
                android:id="@+id/settings_button"
                android:text="..."
                android:textColor="@android:color/white"
                android:clickable="true"
                android:singleLine="false" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:text="O"
                android:id="@+id/O"
                android:layout_centerInParent="true"
                android:background="@drawable/vowelkey"
                android:clickable="false"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:hint="O-" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight"
            android:id="@+id/linearLayout12" >

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout20" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="@integer/key_weight"
                android:text="E"
                android:id="@+id/_E"
                android:layout_centerInParent="true"
                android:background="@drawable/vowelkey"
                android:clickable="false"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:hint="-E" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight"
            android:id="@+id/linearLayout13" >

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/star"
                android:layout_centerInParent="true"
                android:background="@drawable/numberbar"
                android:clickable="false"
                android:text="*"
                android:layout_weight="1"
                android:layout_marginRight="2dp"
                android:hint="*"
                android:layout_marginLeft="2dp" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout18" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:text="U"
                android:id="@+id/_U"
                android:layout_centerInParent="true"
                android:background="@drawable/vowelkey"
                android:clickable="false"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:hint="-U" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:id="@+id/linearLayout5"
            android:layout_weight="@integer/key_weight">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_F"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="F"
                android:hint="-F" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout13" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_R"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:clickable="false"
                android:text="R"
                android:layout_weight="1"
                android:hint="-R" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:id="@+id/linearLayout6"
            android:layout_weight="@integer/key_weight">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_P"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="P"
                android:hint="-P" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout14" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_B"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:clickable="false"
                android:text="B"
                android:layout_weight="1"
                android:hint="-B" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:id="@+id/linearLayout7"
            android:layout_weight="@integer/key_weight">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_L"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="L"
                android:hint="-L" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout15" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_G"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:clickable="false"
                android:text="G"
                android:layout_weight="1"
                android:hint="-G" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight"
            android:id="@+id/linearLayout8">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_T"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="T"
                android:hint="-T" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout16" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_S"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:clickable="false"
                android:text="S"
                android:layout_weight="1"
                android:hint="-S" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight"
            android:id="@+id/linearLayout9">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_D"
                android:layout_centerInParent="true"
                android:background="@drawable/topkey"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="D"
                android:hint="-D" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:id="@+id/frameLayout17" />

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/_Z"
                android:layout_centerInParent="true"
                android:background="@drawable/bottomkey"
                android:layout_marginRight="2dp"
                android:layout_marginLeft="2dp"
                android:clickable="false"
                android:layout_weight="1"
                android:text="Z"
                android:hint="-Z" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:minHeight="100dp"
            android:layout_weight="@integer/key_weight"
            android:id="@+id/linearLayout14">

            <Button
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/send_button"
                android:layout_centerInParent="true"
                android:background="@drawable/sendbutton"
                android:cropToPadding="true"
                android:clickable="false"
                android:text="S         E         N         D"
                android:layout_marginLeft="4dp" />

        </LinearLayout>

    </LinearLayout>


</com.brentandjody.stenokeyboard.TouchLayer>
